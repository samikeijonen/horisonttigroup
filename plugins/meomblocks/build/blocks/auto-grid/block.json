{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "meomblocks/auto-grid",
  "version": "0.1.0",
  "title": "Sarakkeet (automaattinen gridi)",
  "category": "meomblocks",
  "keywords": [
    "image",
    "media",
    "content",
    "meomblocks"
  ],
  "description": "Sarakkeet menevät automaattisesti seuraavalle riviin, jos niillä ei ole tilaa.",
  "attributes": {
    "columnCount": {
      "type": "number",
      "default": 3
    },
    "gap": {
      "type": "string",
      "default": "gap-default"
    }
  },
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "text": false,
      "background": false
    },
    "spacing": {
      "margin": [
        "top"
      ]
    }
  },
  "textdomain": "meomblocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}